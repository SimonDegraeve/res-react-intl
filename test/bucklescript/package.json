{
  "name": "bucklescript",
  "version": "0.0.0",
  "scripts": {
    "build": "bsb -clean-world -make-world",
    "watch": "bsb -make-world -w",
    "clean": "bsb -clean-world",
    "test": "jest --passWithNoTests",
    "start": "yarn test",
    "extract": "../../_build/default/bin/Extract.exe --allow-duplicates src"
  },
  "dependencies": {
    "@glennsl/bs-jest": "^0.5.1",
    "bs-platform": "8.3.0",
    "bs-react-intl": "^2.0.0",
    "bs-react-testing-library": "^0.7.3",
    "jest": "^26.4.2",
    "minimist": "^1.2.5",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-intl": "^5.8.3",
    "reason-react": "^0.9.1"
  },
  "jest": {
    "testMatch": [
      "**/src/**/*_test.bs.js"
    ]
  }
}
